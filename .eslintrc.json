{
  "extends": "next/core-web-vitals",
  "plugins": [
    "tailwindcss"
  ],
  "rules": {
    "no-unused-vars": "error",
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      "tab"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "react/react-in-jsx-scope": "off",
    "no-console": "warn",
    "eqeqeq": [
      "error",
      "always"
    ],
    "tailwindcss/classnames-order": "warn"
  }
}
